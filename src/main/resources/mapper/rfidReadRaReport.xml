<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zimax.mcrs.report.mapper.RFIDReadRaReportMapper">

    <!--添加RFID读取率报表-->
    <insert id="addRFIDReadRa" parameterType="com.zimax.mcrs.report.pojo.RFIDReadRa">
        insert into rep_rfid
        values (#{RFIDReadId}, #{equipmentId}, #{RFIDId}, #{antennaId}, #{readRate},
                #{recordTime})
    </insert>


</mapper>