<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zimax.components.coframe.dict.mapper.DictLoaderMapper">

    <!--通过字典类型编号dictTypeId加载字典数据-->
    <select id="getDictDatas" resultType="com.zimax.components.coframe.dict.pojo.DictData">
        select dictId, dictName from dict_entry where dictTypeId = #{dictTypeId}
    </select>

</mapper>