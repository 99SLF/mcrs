<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zimax.mcrs.update.mapper.UpdateUploadMapper">
    <!--添加更新包信息-->
    <insert id="addUpdateUpload" parameterType="com.zimax.mcrs.update.pojo.UpdateUpload">
        insert into upd_upload
        values (#{uploadId}, #{uploadNumber}, #{version}, #{deviceSoType}, #{uploadStrategy},
                #{fileName}, #{uploader}, #{versionUploadTime}, #{remarks}, #{majorVersion}, #{downloadUrl})
    </insert>
</mapper>